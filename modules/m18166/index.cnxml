              <m:mspace width="4pt"/>
              <m:mspace width="4pt"/>
              <m:mo>+</m:mo>
              <m:mspace width="4pt"/>
              <m:mspace width="4pt"/>
              <m:mn>1</m:mn>
              <m:mo>)</m:mo>
