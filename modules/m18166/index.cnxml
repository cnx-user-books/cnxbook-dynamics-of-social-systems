            <m:mo>=</m:mo>
            <m:mspace width="4pt"/>
            <m:mspace width="4pt"/>
            <m:mspace width="4pt"/>
            <m:msup>
              <m:mi>A</m:mi>
              <m:mi>n</m:mi>
            </m:msup>
            <m:mspace width="4pt"/>
            <m:munder>
              <m:mi>P</m:mi>
              <m:mo>̲_</m:mo>
            </m:munder>
            <m:mspace width="0.166667em"/>
