            <m:mspace width="4pt"/>
          </m:mrow>
        </m:math>
      </equation>
      
      <equation id="id2258558">
        <m:math mode="display">
          <m:mrow>
            <m:msub>
              <m:mover accent="true">
                <m:mi>p</m:mi>
